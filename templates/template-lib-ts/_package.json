{
  "name": "lib",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "commit": "cz",
    "build": "tsup src/index.ts --dts --format esm,cjs",
